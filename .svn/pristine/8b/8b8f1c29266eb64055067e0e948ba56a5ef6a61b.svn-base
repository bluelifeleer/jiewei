/*自定义使用的类*/
.mesg-box{position:relative;}
.mesg-badge{display:inline-block;width:0.8rem;height:0.8rem;border-radius:0.4rem;background:#FF0000;z-index:999;position:absolute;left:0.9rem;top:0.2rem;color:#FFF;text-align:center;line-height:0.8rem;box-sizing:border-box}
.card-badge{width:100%;height:0.5rem;border-top-left-radius:0.2rem;border-top-right-radius:0.2rem;}
.card-badge-no{background:#0082FE;}
.card-badge-off{background:#FFF;}
.mesg-title{width:100%;}
.mesg-time{width:100%;color:#c1c1c1;}
.mesg-content{width:100%;}
.card-content-title{font-size:0.8rem;font-weight:bold;color:#000;}
.product-tab-button{display:inline-block;height:100%;line-height:2.5rem;}
#go-to-top-block{pisotion:fixed;bottom:0;right:0;z-index:100000;}
.go-to-top-mask-layer{background:#FFF;opacity:0.6;box-shadow:0 0 3px #707071;}
.picker-class{background:#ECECEC !important;color:#3B81FD !important}
