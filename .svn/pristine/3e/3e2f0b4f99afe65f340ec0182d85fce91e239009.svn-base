header{background-color: #CB1408 !important;}
.bar::after{height: 0 !important;}
header .title{color: #fff !important;}
ul,.list-block{margin: 0;}
img{display: block !important;}
header a:hover{color: #fff !important;}
.bar-tab .tab-item .icon{font-size: 1rem !important;}
.bar-tab a:hover{color: #929292 !important;}
.bar-tab .tab-item.active, .bar-tab .tab-item:active{color: #fa6a0b !important;}
p{margin: 0;padding: 0;}

/* 字体库 */
@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_1458094203_8168268.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/font_1458094203_8168268.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/font_1458094203_8168268.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/font_1458094203_8168268.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/font_1458094203_8168268.svg#iconfont') format('svg'); /* iOS 4.1- */
}              
                    
                    
.iconfont{
  font-family: "iconfont" !important;
  font-size: 1rem;
  margin: 0 1rem;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

/* 渐变 */
.gradient{
  background-color: #a49cb4;
  /* IE 8+ */
  background: -webkit-gradient(linear,left top,right top,from(#eee),to(#a49cb4));
  /* Firefox 3.6+ */
  background: -moz-linear-gradient(left,#eee,#a49cb4);
  /* Opera 11.10+ */
  background: -o-linear-gradient(left,#eee,#a49cb4);
  /* IE 10+ */
  background: -ms-linear-gradient(left,#eee,#a49cb4);
}

/* 透明 */
.opacity{background:rgba(0, 0, 0, 0.8);}

/* 定位 */
.pos-f{position:fixed;}/* 固定 */
.pos-r{position:relative;}/* 相对 */
.pos-a{position:absolute;}/* 绝对 */

/* 公告样式 */
.clear{clear:both;}
.fll{float:left;}
.flr{float:right;}
.txal{text-align:left;}
.txar{text-align:right;}
.txac{text-align:center;}
.wwp{word-wrap:break-word;}/*英文自动换行*/
.disb{display: block !important;}
.disi{display: inline !important;}
.disn{display: none !important;}
.box_bor{box-sizing:border-box;}
.textove{
  overflow:hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.input_c .input_title{
  line-height:1.5rem;
  font-size:0.7rem;
  color:#ccc;
}
.input_c .input_text input{
  color:#666;
  height:1.5rem;
  font-size:0.7rem;
}
.input_c .input_text select{
  color:#666;
  height:1.5rem;
  font-size:0.7rem;
}
#product_content p,#product_content img{
  padding: 0 !important;
  margin: 0 !important;
  width: 100% !important;
}

