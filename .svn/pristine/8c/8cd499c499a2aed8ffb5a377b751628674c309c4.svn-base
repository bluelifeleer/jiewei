    <body>
        <div class="page" id="page-manage-add-goods">
            <header class="bar bar-nav">
                <a class="pull-left back" href="shop_product_add.html">
                    <i class="icon iconfont mar0 white">&#xe61b;</i>
                </a>
                <h1 class="title">导入商品</h1>
            </header>　

            <!--这里是内容区-->
            <div class="content">
                <div class="content-block-title">导入共享库商品</div>
                <div class="list-block">
                  <ul id="cate-list-block">

                  </ul>
                  <div class="list-block-label">当前一共导入<font id="pro_number">0</font>款产品</div>
                  <div id="open-shop" class="block w80b marauto h50 lh50 txac white bgCB1408 mart20 borrad5">添加</div>
                </div>
       
            </div>
  
        </div>
         
            <!--这里是内容区-->


            <div class="popup popup-product-import" id="popup-product-import">

                   <div class="page" id="popup-page-product-import">
                      <header class="bar bar-nav">
                        <a class="pull-left external  shop-product-list-cate"  href="javascript:void(0);">
                          <i class="icon iconfont mar0 white fs13">&#xe630;</i>
                        </a>
                        <a class="button button-link button-nav pull-right close-popup close-panel">
                          关闭
                        </a>
                        <h1 class="title close-panel">选择导入商品</h1>
                      </header>
                      <nav class="bar bar-tab">
                        <div class="w100b h40 lh40 ovfh bgfff borb1 bcF7F7F8">
                          <span class="select-all-but block fll w80 h30 lh30 fs06 white bgCB1408 borrad5 txac marl20 mart5" data-is-selected="false">全选</span>
                          <span class="random-select-but block fll w80 h30 lh30 fs06 white bgCB1408 borrad5 txac marl20 mart5">随机选择</span>
                          <span class="block fll w80 h30 lh30 fs06 white bgCB1408 borrad5 txac marl20 mart5" data-id="" id="finish-add-goods-but" href="javascript:void(0);">导入</span>
                        </div>
                      </nav>
                      <div class="content bgEEEEEE infinite-scroll close-panel" data-distance="100">
                       
                          <div class="list-container"></div>
                          <!-- 加载提示符 -->
                          <div class="infinite-scroll-preloader">
                              <div class="preloader"></div>
                              <div class="preloader_null disn fs06">没有更多的商品了</div>
                          </div>
                       
                      </div>
                    </div>
            </div>
            
          
    </body>
